<p id="notice"><%= notice %></p>

<h1>Documento Identidads</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Descripcion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @documento_identidads.each do |documento_identidad| %>
      <tr>
        <td><%= documento_identidad.nombre %></td>
        <td><%= documento_identidad.descripcion %></td>
        <td><%= link_to 'Show', documento_identidad %></td>
        <td><%= link_to 'Edit', edit_documento_identidad_path(documento_identidad) %></td>
        <td><%= link_to 'Destroy', documento_identidad, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Documento Identidad', new_documento_identidad_path %>
