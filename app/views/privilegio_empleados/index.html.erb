<p id="notice"><%= notice %></p>

<h1>Privilegio Empleados</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Descripcion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @privilegio_empleados.each do |privilegio_empleado| %>
      <tr>
        <td><%= privilegio_empleado.nombre %></td>
        <td><%= privilegio_empleado.descripcion %></td>
        <td><%= link_to 'Show', privilegio_empleado %></td>
        <td><%= link_to 'Edit', edit_privilegio_empleado_path(privilegio_empleado) %></td>
        <td><%= link_to 'Destroy', privilegio_empleado, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Privilegio Empleado', new_privilegio_empleado_path %>
