<%= form_with(model: empleado, local: true) do |form| %>
  <% if empleado.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(empleado.errors.count, "error") %> prohibited this empleado from being saved:</h2>

      <ul>
      <% empleado.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= form.label :documento_id %>
    <!-- <%= form.number_field :documento_id %> -->
    <%= form.collection_select :id_tipo_documento, Documento.order(nombre: :asc), :id, :nombre%>
  </div>

  <div class="field">
    <%= form.label :numero_documento %>
    <%= form.text_field :numero_documento %>
  </div>

  <div class="field">
    <%= form.label :nombre %>
    <%= form.text_field :nombre %>
  </div>

  <div class="field">
    <%= form.label :apellido_paterno %>
    <%= form.text_field :apellido_paterno %>
  </div>

  <div class="field">
    <%= form.label :apellido_materno %>
    <%= form.text_field :apellido_materno %>
  </div>

  <div class="field">
    <%= form.label :privilegio_id %>
    <!-- <%= form.number_field :privilegio_id %> --->
    <%= form.collection_select :id_tipo_documento, Privilegio.order(nombre: :asc), :id, :nombre%>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
